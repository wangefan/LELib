<?xml version="1.0" encoding="UTF-8"?>
<Commands>
    <CmdGroup Title="DEVICE SETTINGS">
        <WriteCmd Title="RESET" Cmd="reset">
			<CmdRes>ok</CmdRes>
		</WriteCmd>
		<ReadCmd Title="INFO" Cmd="man">
			<CmdRes>man Integral</CmdRes>
		</ReadCmd>
    </CmdGroup>
	<CmdGroup Title="CHANNEL SELECTION">
        <WriteCmd Title="CHANNEL/INPUT 1" Cmd="insel 1">
			<CmdRes>ok</CmdRes>
		</WriteCmd>
		<WriteCmd Title="CHANNEL/INPUT 2" Cmd="insel 2">
			<CmdRes>ok</CmdRes>
		</WriteCmd>
		<ReadCmd Title="Read Channel" Cmd="insel">
			<CmdRes>insel 1</CmdRes>
			<CmdRes>insel 2</CmdRes>
		</ReadCmd>
    </CmdGroup>
	<CmdGroup Title="HDCP SETTINGS">
	    <WriteCmd Title="HDCP 1.4 only" Cmd="hdcp 14">
			<CmdRes>ok</CmdRes>
		</WriteCmd>
        <WriteCmd Title="HDCP 2.2 + 1.4" Cmd="hdcp 22">
			<CmdRes>ok</CmdRes>
		</WriteCmd>
		<ReadCmd Title="Read HDCP" Cmd="hdcp">
			<CmdRes>hdcp 14</CmdRes>
			<CmdRes>hdcp 22</CmdRes>
		</ReadCmd>
    </CmdGroup>	
    <CmdGroup Title="SOUND SETTINGS">
        <WriteCmd Title="EXTERNAL AUDIO," Cmd="audio jack">
			<CmdRes>ok</CmdRes>
		</WriteCmd>
        <WriteCmd Title="FROM HDMI" Cmd="audio hdmi">
			<CmdRes>ok</CmdRes>
		</WriteCmd>
		<ReadCmd Title="Read Audio" Cmd="audio">
			<CmdRes>audio hdmi</CmdRes>
			<CmdRes>audio jack</CmdRes>
		</ReadCmd>
    </CmdGroup>
	<CmdGroup Title="EDID SETTINGS">
	    <WriteCmd Title="AUTO MIX MODE" Cmd="edid automix">
			<CmdRes>ok</CmdRes>
		</WriteCmd>
		<WriteCmd Title="CUSTOM" Cmd="edid custom">
			<CmdRes>ok</CmdRes>
		</WriteCmd>
        <WriteCmd Title="Fixed with L/R stereo" Cmd="edid fixed">
			<CmdRes>ok</CmdRes>
		</WriteCmd>
		<ReadCmd Title="Read EDID" Cmd="edid">
			<CmdRes>edid fixed</CmdRes>
			<CmdRes>edid custom</CmdRes>
			<CmdRes>edid automix</CmdRes>
		</ReadCmd>
    </CmdGroup>
	
</Commands>